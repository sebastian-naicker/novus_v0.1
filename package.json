  "description": "Starting point for all javascript projects",
  "scripts": {
    "start": "npm-run-all --parallel start:server",
    "start:server": "webpack-dev-server --config .webpack/dev.config.babel.js",
    "security-check": "npm audit",
    "clean:dist": "rimraf ./build && mkdir build && rimraf /var/www/novus/*",
    "prebuild": "npm-run-all clean:dist",
    "build": "webpack -p --config .webpack/prod.config.babel.js",
