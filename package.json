{
  "name": "jsde-sandbox",
  "version": "0.1.0",
  "description": "Starting point for all projects",
  "scripts": {
    "start": "npm-run-all --parallel security-check start:server",
    "start:server": "webpack-dev-server --config webpack/dev.config.babel.js",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3003",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "prerelease": "npm-run-all clean:dist",
    "release": "webpack -p --config webpack/prod.config.babel.js"
  },
  "author": "Sebastian Naicker",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^8.3.0",
    "babel-polyfill": "^6.26.0",
    "extract-loader": "^2.0.1",
    "postcss-loader": "^2.1.4",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-essentials": "^1.1.1",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader2": "^1.1.2",
    "jest": "^22.1.4",
    "jsdom": "^11.5.1",
    "json-server": "^0.12.1",
    "localtunnel": "^1.8.3",
    "nock": "^9.1.6",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-md5-hash": "^0.0.6"
  }
}
