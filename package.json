{
  "name": "jsde-sandbox",
  "version": "0.1.0",
  "description": "Starting point for all projects",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check start:server lint:watch test:watch start:mockapi",
    "start:server": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3003",
    "start:lt": "npm-run-all --parallel start:server localtunnel",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generateMockData": "babel-node buildScripts/generateMockData.js",
    "prestart:mockapi": "npm run generateMockData",
    "start:mockapi": "json-server --watch src/api/db.json --port 3002",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "prerelease": "npm-run-all clean:dist test lint",
    "release": "babel-node buildScripts/build.js",
    "postrelease": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "author": "Sebastian Naicker",
  "license": "MIT",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.15.0",
    "eslint-config-essentials": "^1.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "json-schema-faker": "^0.5.0-rc12",
    "json-server": "^0.12.1",
    "localtunnel": "^1.8.3",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "surge": "^0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-md5-hash": "^0.0.6"
  }
}
